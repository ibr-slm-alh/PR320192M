<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8">
   <children>
      <BorderPane layoutX="14.0" prefHeight="271.0" prefWidth="267.0">
         <bottom>
            <Button fx:id="buttonUpdate" mnemonicParsing="false" onAction="#handleButtonUpdate" text="Update" BorderPane.alignment="CENTER" />
         </bottom>
         <center>
            <TextField fx:id="textFieldName" BorderPane.alignment="CENTER" />
         </center>
         <top>
            <ListView fx:id="ListViewNames" onMouseClicked="#handleListViewNames" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
         </top>
      </BorderPane>
   </children>
</AnchorPane>
